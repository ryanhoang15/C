#include <iostream>
using namespace std;

int main() {
  
  int number = 9;
  
  switch(number) {
    
    case 1 :
      cout << "Bulbusaur\n";
      break;
    case 2 :
      cout << "Ivysaur\n";
      break;
    case 3 :
      cout << "Venusaur\n";
      break;
    case 4 :
      cout << "Charmander\n";
      break;
    case 5 :
      cout << "Charmeleon\n";
      break;
    case 6 :
      cout << "Charizard\n";
      break;
    case 7:
      cout << "Squirtle\n";
      break;
    case 8:
      cout << "Wartortle\n";
      break;
    case 9:
      cout << "Blastoise\n";
//    default :
//      cout << "Unknown\n";
//      break;
  
  }
  
}
